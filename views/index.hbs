<section class="ftco-section ftco-consult ftco-no-pt ftco-no-pb" style="background-image: url(/images/bg_5.jpg);"
    data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row justify-content-end">
            <div class="col-md-6 py-5 px-md-5">
                <div class="py-md-5">
                    <div class="heading-section heading-section-white ftco-animate mb-5">
                        <h2 class="mb-4">Admission Form Application</h2>
                        <p>Please do well to fill out this form correctly
                        </p>
                    </div>
                    <form action="/user/enroll" class="appointment-form ftco-animate" enctype="multipart/form-data">
                        <div class="d-md-flex">
                            <div class="form-group">
                                <input type="text" name="firstName" class="form-control" placeholder="First Name">
                                <p><noscript></noscript>{{firstName}}</p>

                            </div>
                            <div class="form-group ml-md-4">
                                <input type="text" name="lastName" class="form-control" placeholder="Last Name">
                                <p>{{lastName}}</p>
                            </div>
                        </div>
                        <div class="d-md-flex">
                            <div class="form-group">
                                <input type="text" name="parentName" class="form-control" placeholder="Parent FullName">
                                <p>{{parentName}}</p>
                            </div>
                            <div class="form-group ml-md-4">
                                <input type="tel" name="phone" class="form-control" placeholder="Parent PhoneNumber">
                                <p>{{ParentphoneNumber}}</p>
                            </div>
                        </div>
                        <div class="d-md-flex">
                            <div class="form-group ml-md-4">
                                <input type="text" name="email" class="form-control" placeholder="Email">
                                <p>{{email}}</p>
                            </div>
                           

                            <div class="form-group ml-md-4">
                                <input type="text" name="address" class="form-control" placeholder="Address">
                                <p>{{address}}</p>
                            </div>

                        </div>
                        <div class="form-group d-md-flex">

                            <input type="submit" value="Apply" class="btn btn-primary btn-sm my-2 p-2">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>